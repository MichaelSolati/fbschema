{
  "$schema": "https://raw.githubusercontent.com/MichaelSolati/fbschema/main/fbschema.json",
  "title": "Integer Collection",
  "type": "object",
  "required": ["simple"], 
  "fbschema": {
    "read": true,
    "get": true,
    "list": true,
    "write": true,
    "create": true,
    "update": true,
    "delete": true
  },
  "properties": {
    "simple": {
      "type": "integer"
    },
    "minimum": {
      "type": "integer",
      "minimum": 3
    },
    "exclusiveMinimum": {
      "type": "integer",
      "exclusiveMinimum": 3
    },
    "maximum": {
      "type": "integer",
      "maximum": 10
    },
    "exclusiveMaximum": {
      "type": "integer",
      "exclusiveMaximum": 10
    },
    "multipleOf": {
      "type": "integer",
      "multipleOf": 10
    },
    "range": {
      "type": "integer",
      "minimum": 0,
      "maximum": 100
    },
    "enum": {
      "type": "integer",
      "enum": [
        1,
        2,
        3
      ]
    }
  }
}