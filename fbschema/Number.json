{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Number Collection",
  "type": "object",
  "required": ["simple"], 
  "fbschema": {
    "read": true,
    "get": true,
    "list": true,
    "write": true,
    "create": true,
    "update": true,
    "delete": true
  },
  "properties": {
    "simple": {
      "type": "number"
    },
    "minimum": {
      "type": "number",
      "minimum": 3
    },
    "exclusiveMinimum": {
      "type": "number",
      "exclusiveMinimum": 3
    },
    "maximum": {
      "type": "number",
      "maximum": 10
    },
    "exclusiveMaximum": {
      "type": "number",
      "exclusiveMaximum": 10
    },
    "multipleOf": {
      "type": "number",
      "multipleOf": 0.5
    },
    "range": {
      "type": "number",
      "minimum": 0,
      "maximum": 100
    },
    "enum": {
      "type": "number",
      "enum": [
        1.1,
        2.2,
        3.3
      ]
    }
  }
}